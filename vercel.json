{
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/keep-alive",
      "schedule": "0 */6 * * *"
    }
  ],
  "functions": {
    "src/app/api/cleanup/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/cleanup/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/keep-alive/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/upload/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/upload-chunk/route.ts": {
      "maxDuration": 300
    },
    "src/app/manifest.ts": {
      "maxDuration": 10
    }
  }
}